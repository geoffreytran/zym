<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services
                        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="security.acl.object_identity_retrieval_strategy.class">Zym\Bundle\SecurityBundle\Acl\Domain\ObjectIdentityRetrievalStrategy</parameter>
    </parameters>

    <services>
        <service id="security.acl.collection_cache" class="%security.acl.collection_cache.class%">
            <argument type="service" id="security.acl.provider" />
            <argument type="service" id="security.acl.object_identity_retrieval_strategy" />
            <argument type="service" id="security.acl.security_identity_retrieval_strategy" />
        </service>
    </services>
</container>
